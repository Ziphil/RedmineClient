{
  "name": "redmine-client",
  "version": "1.0.0",
  "author": "Ziphil",
  "main": "dist/index.js",
  "scripts": {
    "develop": "cross-env NODE_ENV=development webpack --watch --mode development --config webpack-develop.ts",
    "build": "webpack --mode production --config webpack-product.ts",
    "lint": "eslint source/**/*.ts source/**/*.tsx",
    "lint:fix": "eslint --fix source/**/*.ts source/**/*.tsx"
  },
  "eslintConfig": {
    "extends": [
      "ziphil",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "webpack-*.ts"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            "internal",
            "parent",
            "sibling",
            "index",
            "object"
          ],
          "pathGroups": [
            {
              "pattern": "/renderer/**",
              "group": "internal"
            },
            {
              "pattern": "/main/**",
              "group": "internal"
            }
          ],
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": false
          },
          "newlines-between": "never"
        }
      ]
    }
  },
  "dependencies": {
    "@holiday-jp/holiday_jp": "^2.4.0",
    "@linaria/babel-preset": "^5.0.4",
    "@linaria/core": "^5.0.2",
    "@linaria/react": "^5.0.3",
    "axios": "^1.6.2",
    "axios-case-converter": "^1.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "electron": "^27.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "simplebar-react": "^3.2.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@linaria/webpack-loader": "^5.0.4",
    "@linaria/webpack5-loader": "^5.0.4",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron-reload-webpack-plugin": "^2.0.4",
    "eslint": "^8.17.0",
    "eslint-config-ziphil": "^1.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "volta": {
    "node": "18.19.0"
  }
}
